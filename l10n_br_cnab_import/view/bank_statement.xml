<?xml version="1.0" ?>
<openerp>
    <data>

        <record id="view_bank_statement_form_inherit" model="ir.ui.view">
            <field name="name">Bank Statements</field>
            <field name="model">account.bank.statement</field>
            <field name="inherit_id" ref="account.view_bank_statement_form" />
            <field name="arch" type="xml">
                <field name="balance_end_real" position="after">
                    <field name="statement_type"/>
                </field>
                <page string="Transactions" position="after" >
                    <page string="CNAB Lines" readonly="1" attrs="{'invisible': [('statement_type','!=','c')]}">
                        <field name="cnab_lines" nolabel="1" >
                            <tree>
                                <field name="date"/>
                                <field name="name"/>
                                <field name="partner_id"/>
                                <field name="ref"/>
                                <field name="transaction_id"/>
                                <field name="unique_import_id"/>
                            </tree>
                        </field>
                    </page>
                </page>
            </field>
        </record>


    </data>
</openerp>
