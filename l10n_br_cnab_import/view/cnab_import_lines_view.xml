<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="cnab_import_lines_form" model="ir.ui.view">
            <field name="name">CNAB Lines</field>
            <field name="model">cnab.lines</field>
            <field name="arch" type="xml">
                <form string="CNAB Lines">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="partner_id"/>
                            <field name="account_no"/>
                            <field name="amount"/>
                            <field name="date"/>
                            <field name="ref"/>
                            <field name="statement_id"/>
                            <field name="transaction_id"/>
                            <field name="error_message"/>
                            <field name="unique_import_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="cnab_import_lines_tree" model="ir.ui.view">
            <field name="name">CNAB Lines</field>
            <field name="model">cnab.lines</field>
            <field name="arch" type="xml">
                <tree string="CNAB Lines">
                     <field name="name"/>
                            <field name="partner_id"/>
                            <field name="account_no"/>
                            <field name="amount"/>
                            <field name="date"/>
                            <field name="ref"/>
                            <field name="statement_id"/>
                            <field name="transaction_id"/>
                            <field name="unique_import_id"/>
                </tree>
            </field>
        </record>

        <record id="cnab_import_lines_action" model="ir.actions.act_window">
            <field name="name">CNAB Lines</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cnab.lines</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Now Lines Please Import CNAB Lines
              </p><p>

              </p>
            </field>
        </record>

        <!-- This Menu Item must have a parent and an action -->
        <menuitem id="cnab_import_lines_menu" name="CNAB Lines" parent="account.menu_finance_bank_and_cash" action="cnab_import_lines_action" sequence="10"/>


    </data>
</openerp>